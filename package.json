{
  "name": "mol.color",
  "version": "1.1.0",
  "description": "editable color palette module of mol project",
  "scripts": {
    "dist": "sass scss/color.scss:dist/color.min.css --style compressed",
    "docs": "npm run scss | npm run start",
    "start": "browser-sync start --server 'docs' --files 'docs'",
    "scss": "sass --watch scss/color.scss:docs/color.css --style expanded"
  },
  "keywords": [
    "mol",
    "mol-project",
    "css",
    "sass",
    "scss",
    "scss-module",
    "color",
    "palette"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mol-project/mol.color.git"
  },
  "author": "flkt <flkt.crnpio@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mol-project/mol.color/issues"
  },
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "mol.normalize": "git+https://github.com/mol-project/mol.normalize.git#v.2.0.1"
  }
}
